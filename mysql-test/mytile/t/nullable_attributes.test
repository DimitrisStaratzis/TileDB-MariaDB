--echo simple int

CREATE TABLE t0 (
dim0 int dimension=1 tile_extent="10",
attr0 int NULL
) ENGINE=mytile;

INSERT INTO t0 VALUES (0, 0);
INSERT INTO t0 VALUES (1, null);
SELECT * FROM t0;

INSERT INTO t0 VALUES (2, 2);
INSERT INTO t0 VALUES (3, null);
INSERT INTO t0 VALUES (4, 4);
INSERT INTO t0 VALUES (5, null);
SELECT * FROM t0;

INSERT INTO t0 VALUES (6, 6);
SELECT * FROM t0;
DROP TABLE t0;

